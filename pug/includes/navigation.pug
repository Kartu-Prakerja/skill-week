    .d-flex.justify-content-between.align-items-center
      a(href='/')
        img(src=baseURL+'img/logo-indonesia-skills-week.webp' height='54' alt='Indonesia Skills Week')
      ul.d-flex.align-items-center.navbar-custom
        li.nav-item.md-hide
          a.nav-link(href='/' class={"nav-link-active" : pageActive === ''}) Beranda
