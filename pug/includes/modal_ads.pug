<!-- Modal Ads -->
//- Ukuran gambar 512x640 px


#adsModal.modal.fade.show(aria-hidden='true' aria-labelledby='adsModal' tabindex='-1')
  .modal-dialog.modal-dialog-centered.pra-modal
    .modal-content
      .modal-header
        h6.modal-title 
          | Siap untuk Scale up?
          br
          | Skill up dengan Webinar #JadiBisa
        button.btn-close(type='button' data-bs-dismiss='modal' aria-label='Close')
      .modal-body.px-lg-5
        a(href="https://bit.ly/webinar-jadi-bisa")
          img.w-100(src="https://mmgfnw.csb.app/popup-banner/isw-popup-banner-2.webp", alt="Prakerja Indonesia Skillsweek") 


      .modal-footer.d-flex.justify-content-center
        a.btn.btn-primary(href="https://bit.ly/webinar-jadi-bisa" target="_blank") 
          | Daftar Webinar Sekarang