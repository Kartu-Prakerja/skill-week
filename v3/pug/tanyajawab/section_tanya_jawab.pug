                                            br

                                            h6 Sudah Login + truncate + title
                                            button.btn.btn-primary.text-truncate.max-width-200(data-bs-toggle="modal" data-bs-target="#loginModal" title="bambang_soehardjo@gmail.com")
                                                i.bi.bi-person-circle.me-2 
                                                | bambang_soehardjo@gmail.com
                                            
                                        li.mb-3
                                            button.btn.btn-primary(data-bs-toggle="modal" data-bs-target="#loginSuccessModal")
                                                
                                                | Success Message

                                        li.mb-3
                                            button.btn.btn-primary(data-bs-toggle="modal" data-bs-target="#loginErrorModal")
                                                
                                                | Error States 

                                        li.mb-3
                                            button.btn.btn-primary(data-bs-toggle="modal" data-bs-target="#loginAlreadyModal")
                                                
                                                | Sudah Login

                                        


                                    hr 
                                    h3 Course
                                    ol 
                                        li.mb-3
                                            button.btn.btn-primary(data-bs-toggle="modal" data-bs-target="#getCourseNotLoginModal")
