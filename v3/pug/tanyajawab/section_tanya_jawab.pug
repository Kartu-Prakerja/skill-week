                                        li.mb-3
                                            button.btn.btn-primary(data-bs-toggle="modal" data-bs-target="#getCourseLoginModal")
                                                | Ambil Pelatihan - Sudah Login

                                        li.mb-3
                                            button.btn.btn-primary(data-bs-toggle="modal" data-bs-target="#getFreeCourseNotLoginModal")
                                                | Ambil Pelatihan Gratis - Belum Login

                                        li.mb-3
                                            button.btn.btn-primary(data-bs-toggle="modal" data-bs-target="#getFreeCourseLoginModal")
                                                | Ambil Pelatihan Gratis - Sudah Login

                                        li.mb-3
                                            button.btn.btn-primary(data-bs-toggle="modal" data-bs-target="#getCourseFailedModal")
                                                | Ambil Pelatihan - Sudah Login - Gagal

                                        li.mb-3 
                                            button#liveToastBtn.btn.btn-primary(type='button') Toast Sukses Ambil Pelatihan

                                    
                            
